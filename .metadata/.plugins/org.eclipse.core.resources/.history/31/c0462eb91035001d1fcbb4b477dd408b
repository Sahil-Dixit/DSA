package com.sahil;

public class LinkedList {

	private Node head;
	private Node tail;
	private int size;

	private class Node {
		private int value;
		private Node next;

		public Node(int value) {
			this.value = value;
		}

		public Node(int value, Node next) {
			this.value = value;
			this.next = next;
		}
	}

	public void insertFirst(int val) {
		Node node = new Node(val);
		node.next = head;
		head = node;
		size++;

	}

	public static void main(String[] args) {
		LinkedList ll = new LinkedList();

	}

}
